{
  "name": "expensify",
  "version": "1.0.0",
  "description": "An application made with React and Redux",
  "main": "index.js",
  "repository": "https://github.com/barryblando/expensify.git",
  "author": "Retr0_0x315 <barryblando@gmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "quokka": {
    "babel": {
      "presets": [
        "env",
        "react"
      ]
    },
    "installPackageCommand": "yarn add {packageName}"
  },
  "engines": {
    "node": "10.3.0",
    "yarn": "1.7.0"
  },
  "scripts": {
    "start": "babel-node buildScripts/prodServer.js",
    "heroku-postbuild": "yarn build:prod",
    "build:dev": "cross-env NODE_ENV=development babel-node buildScripts/devServer.js",
    "build:prod": "cross-env NODE_ENV=production babel-node buildScripts/buildProd.js",
    "serve:dev": "live-server public/",
    "serve:prod": "babel-node buildScripts/prodServer.js",
    "playground:watch": "babel src/playground/counter-example-state.js --out-file=public/scripts/app.js --presets=env,react -w",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "stylelint": "stylelint \"src/styles/**/*.scss\" --config .stylelintrc.json",
    "clean:package": "rm -rf node_modules",
    "clean:cache": "yarn --force cache clean",
    "install:package": "yarn --force install",
    "rebuild:package": "yarn clean:cache && yarn install:package"
  },
  "dependencies": {
    "autoprefixer": "^8.6.2",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chart.js": "^2.7.2",
    "compression": "^1.7.2",
    "compression-webpack-plugin": "^1.1.11",
    "connected-react-router": "^4.3.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "firebase": "^5.0.4",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "numeral": "^2.0.6",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-chartjs-2": "^2.7.2",
    "react-dates": "^16.7.0",
    "react-dom": "^16.4.1",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "validator": "^10.3.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-md5-hash": "^0.0.6"
  },
  "devDependencies": {
    "@types/numeral": "^0.0.22",
    "@types/react": "^16.3.18",
    "@types/react-dates": "^16.7.0",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.5",
    "prettier-stylelint": "^0.4.2",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "signale": "^1.2.1",
    "stylelint": "^9.3.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.3",
    "webpack-dev-server": "3.1.4"
  }
}
